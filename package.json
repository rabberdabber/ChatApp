{
    "name": "chats",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "DEBUG=chats:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=rabb CHATS_MODEL=mongodb USER_SERVICE_URL=http://localhost:3333 node --experimental-modules ./app",
        "start-server1": "DEBUG=chats:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=rabb CHATS_MODEL=mongodb USER_SERVICE_URL=http://localhost:3333 PORT=3000 node --experimental-modules ./app",
        "start-server2": "DEBUG=chats:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=rabb CHATS_MODEL=mongodb USER_SERVICE_URL=http://localhost:3333 PORT=3002 node --experimental-modules ./app",
        "start-sequelize": "MONGO_URL=mongodb://localhost/ MONGO_DBNAME=rabb CHATS_MODEL=mongodb USER_SERVICE_URL=http://localhost:3333 node --experimental-modules ./bin/www.mjs",
        "dl-minty": "mkdir -p minty && npm run dl-minty-css && npm run dl-minty-min-css",
        "dl-minty-css": "wget https://bootswatch.com/4/minty/bootstrap.css -O minty/bootstrap.css",
        "dl-minty-min-css": "wget https://bootswatch.com/4/minty/bootstrap.min.css -O minty/bootstrap.min.css",
        "docker": "node --experimental-modules ./app",
        "docker-build": "docker build -t node-web-development/chats ."
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.1.1",
        "body-parser": "~1.18.2",
        "bootstrap": "^4.6.1",
        "bootstrap-fileinput": "^5.2.8",
        "connect-flash": "^0.1.1",
        "cookie-parser": "~1.4.3",
        "debug": "~2.6.9",
        "express": "^4.18.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.3",
        "feather-icons": ">=4.5.x",
        "font-awesome": "^4.7.0",
        "hbs": "~4.0.1",
        "jquery": "^1.12.4",
        "jquery-fileinput": "^1.3.1",
        "mongodb": "^3.7.3",
        "morgan": "~1.9.0",
        "passport": "^0.6.0",
        "passport-local": "^1.0.0",
        "passport-twitter": "^1.0.4",
        "passport.socketio": "^3.7.0",
        "popper.js": "^1.16.1",
        "serve-favicon": "^2.5.0",
        "session-file-store": "^1.5.0",
        "socket.io": "^4.5.1",
        "superagent": "^7.1.6"
    }
}
